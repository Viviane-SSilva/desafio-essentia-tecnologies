
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id Int @id @default(autoincrement())
  title  String
  description String?
  completed Boolean @default(false)
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt

  @@map("task")
}